	<subDataset name="dbChiTietLuongCN" uuid="410fe6b3-54c8-45d8-b24a-8bd37329b16b">
		<parameter name="maCN" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{maCN}]]></defaultValueExpression>
		</parameter>
		<parameter name="thang" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[$P{thang}]]></defaultValueExpression>
		</parameter>
		<parameter name="nam" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[$P{nam}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select'Ngày làm'=DAY(DD.ngayLam),'Ca làm' = Case when coMat != 2 then 0
		else  caLam end,
'Có mặt' = Case coMat 
when 0 then N'Không phép'
when 1 then N'Có phép'
else N'Có mặt'
end, CD.donGia, 'Số lượng' = Case when coMat != 2 then 0
		else  SP.soLuongSP end,
'Loại ngày' = CASE loaiNgay
WHEN 1 THEN N'Ngày lễ'
		
ELSE case DATEPART(w, NL.ngayLam)  
	When '1' Then N'Chủ nhật'
	Else N'Ngày thường' 
	END
END,
		'Lương ca' = Case when coMat != 2 then 0
		else 
		Convert(int,Round(Case caLam
							When 3 Then 
										CASE WHEN  (select soLuongSP from BangChamCongSP where maCCCN = DD.maCCCN AND caLam = 1) > 0 and (select soLuongSP from BangChamCongSP where maCCCN = DD.maCCCN AND caLam = 2) > 0 THEN 
														CASE loaiNgay  WHEN 1 THEN soLuongSP * donGia * 4 
														Else CASE
																WHEN DATEPART(w, NL.ngayLam) = '1' THEN  soLuongSP*donGia * 3
																ELSE   2*soLuongSP* donGia
															END
														END
										END
							Else case loaiNgay  
									WHEN 1 THEN 3*soLuongSP * donGia 
									Else CASE
											WHEN DATEPART(w, NL.ngayLam) = '1' THEN  2*soLuongSP*donGia
											ELSE   1*soLuongSP* donGia
										END
									END
					End,0))
					end
	From CongNhan CN, CongDoan CD, BangPhanCong PC, BangDiemDanhCN DD left Join BangChamCongSP SP ON DD.maCCCN=SP.maCCCN, NgayLamViec NL
	Where CN.maCN = PC.maCN AND CD.maCD = PC.maCD AND PC.maPC = DD.maPC AND DD.ngayLam = NL.ngayLam 
		AND MONTH(DD.ngayLam)=$P{thang} AND YEAR(DD.ngayLam)=$P{nam} and CN.maCN =$P{maCN}
	Order by NL.ngayLam]]>
		</queryString>
		<field name="Ngày làm" class="java.lang.Integer"/>
		<field name="Ca làm" class="java.lang.Integer"/>
		<field name="Có mặt" class="java.lang.String"/>
		<field name="donGia" class="java.lang.Double"/>
		<field name="Số lượng" class="java.lang.Integer"/>
		<field name="Loại ngày" class="java.lang.String"/>
		<field name="Lương ca" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="maCN" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="thang" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="nam" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select maLCN,hoTen, CN.maCN, luongSanPham = convert(int,luongSanPham),luongTangCa= convert(int,luongTangCa),BHXH =2800000*CN.bHXH,CN.phuCap, thuong, phat ,thamNien
 = convert(int,CN.thamNien*BL.luongsanpham/100),tongLuong= convert(int,tongLuong) from BangLuongCN BL, CongNhan CN
where thang = $P{thang} and nam = $P{nam} and CN.maCN = BL.maCN and CN.maCN = $P{maCN}]]>
	</queryString>
